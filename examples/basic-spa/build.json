{
  "modularImportRuntime": true,
  "ignoreHtmlTemplate": false,
  "plugins": [],
  "modeConfig": {
    "prod": {
      "ignoreHtmlTemplate": true
    }
  },
  "eslint": {
    "disable": false,
    "quiet": true
  },
  "postcssOptions": {
    "plugins": {
      "postcss-preset-env": {
        "browsers": [
          "chrome 68"
        ]
      }
    }
  },
  "webpackPlugins": {
    "MiniCssExtractPlugin": {
      "options": {
        "filename": "[name]-test.css"
      }
    }
  },
  "webpackLoaders": {
    "scss-module": {
      "loaders": {
        "css-loader": {
          "options": {
            "modules": {
              "localIdentName": "[local]--[hash:base64:7]"
            }
          }
        }
      }
    }
  }
}
