{
  "plugins": [
    [
      "build-plugin-prerender",
      {
        "routes": [
          "/",
          "/about",
          "/dashboard"
        ],
        "minify": {
          "collapseBooleanAttributes": true,
          "collapseWhitespace": true,
          "decodeEntities": true,
          "keepClosingSlash": true,
          "sortAttributes": true
        },
        "renderer": {
          "headless": false
        }
      }
    ]
  ]
}