  "devDependencies": {
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "^3.1.0",
    "@types/less": "^3.0.3",
    "@types/micromatch": "^4.0.2",
    "@types/multer": "^1.4.7",
    "@types/temp": "^0.9.1",
    "esbuild": "^0.17.16",
    "jest": "^29.0.2",
    "react": "^18.2.0",
    "react-router": "6.21.3",
    "sass": "^1.50.0",
    "unplugin": "^1.6.0",
    "webpack": "^5.88.0",
    "webpack-dev-server": "4.15.0",
    "@rspack/core": "0.5.7",
    "@rspack/dev-server": "0.5.7"
  },
