    "commander": "^9.0.0",
    "consola": "^2.15.3",
    "cross-spawn": "^7.0.3",
    "detect-port": "^1.3.0",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "ejs": "^3.1.10",
    "fast-glob": "^3.2.11",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.0",
    "micromatch": "^4.0.5",
    "mlly": "^1.1.0",
    "mrmime": "^1.0.0",
    "open": "^8.4.0",
    "path-to-regexp": "^6.3.0",
    "regenerator-runtime": "^0.13.0",
    "resolve.exports": "^1.1.0",
    "semver": "^7.3.5",
