    "@ice/route-manifest": "workspace:*",
    "@ice/runtime": "workspace:^",
    "@ice/shared-config": "workspace:*",
    "@ice/webpack-config": "workspace:*",
    "@ice/rspack-config": "workspace:*",
    "@swc/helpers": "0.5.1",
    "@types/express": "^4.17.14",
    "address": "^1.1.2",
    "build-scripts": "^2.1.2-0",
    "chalk": "^4.0.0",
