import React from 'react';
import { LineChart } from '@alife/bizcharts-plot';

export default function <%= className %>(props) {
  const {
    data,
    title,
    description,
    point,
    xField,
    yField,
  } = props;
  return (
    <div className="<%= name %>">
      <LineChart
        data={data}
        title={title}
        description={description}
        point={point}
        xField={xField}
        yField={yField}
        forceFit
      />
    </div>
  );
}

<%= className %>.propTypes = {
};

<%= className %>.defaultProps = {
};
