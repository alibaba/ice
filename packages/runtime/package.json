    "watch": "tsc -w --sourceMap",
    "build": "tsc",
    "postinstall": "node ./scripts/postinstall.mjs"
  },
  "devDependencies": {
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "regenerator-runtime": "^0.13.9",
    "@remix-run/web-fetch": "^4.3.3"
