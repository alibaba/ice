{
  "name": "@ice/plugin-i18n",
  "version": "0.1.0",
  "description": "I18n plugin for ice.js 3.",
  "files": [
    "es2017"
  ],
  "type": "module",
  "main": "es2017/index.js",
  "module": "es2017/index.js",
  "types": "es2017/index.d.ts",
  "exports": {
    ".": {
      "types": "./es2017/index.d.ts",
      "import": "./es2017/index.js"
    },
    "./runtime": {
      "types": "./es2017/runtime/index.d.ts",
      "import": "./es2017/runtime/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build"
  },
  "keywords": [
    "ice.js",
    "i18n",
    "plugin"
  ],
  "dependencies": {
    "@swc/helpers": "^0.4.14"
  },
  "devDependencies": {
    "@ice/pkg": "^1.0.0",
    "@ice/app": "^3.1.4",
    "@ice/runtime": "^1.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}