import { IAppConfig } from '../types';

async function loadStaticModules(appConfig: IAppConfig) {
  <% if (runtimeModules.length) {%>
    <% runtimeModules.filter((moduleInfo) => moduleInfo.staticModule).forEach((runtimeModule) => { %>
      (await import('<%= runtimeModule.path %>')).default({ appConfig });
    <% }) %>
  <% } %>
}

export default loadStaticModules;
