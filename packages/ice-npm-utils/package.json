{
  "name": "ice-npm-utils",
  "version": "1.4.0",
  "description": "npm utils for ice",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc",
    "test": "NODE_ENV=unittest LOG_LEVEL=verbose jest",
