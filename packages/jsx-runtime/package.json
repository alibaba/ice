{
  "name": "@ice/jsx-runtime",
  "version": "0.3.1",
  "description": "JSX runtime for ice.",
  "files": [
    "esm",
    "dist",
    "jsx-dev-runtime.js",
    "jsx-runtime.js"
  ],
  "type": "module",
  "main": "./esm/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": "./esm/index.js",
    "./jsx-dev-runtime": "./esm/dev.js",
    "./jsx-runtime": "./esm/prod.js"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "watch": "cross-env NODE_ENV=development ice-pkg start",
    "build": "cross-env NODE_ENV=production ice-pkg build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ice",
    "react",
    "jsx-runtime"
  ],
  "dependencies": {
    "style-unit": "^3.0.4",
    "@swc/helpers": "^0.5.13"
  },
  "devDependencies": {
    "@ice/pkg": "^1.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cross-env": "^7.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
