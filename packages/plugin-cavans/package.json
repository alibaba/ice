{
  "name": "@ice/plugin-canvas",
  "version": "0.0.1",
  "description": "Provide canvas render support for ice.js",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./esm/index.js"
    },
    "./CacheCanvas": {
      "types": "./esm/CacheCanvas.d.ts",
      "import": "./esm/CacheCanvas.js",
      "default": "./esm/CacheCanvas.js"
    },
    "./runtime": {
      "types": "./esm/runtime.d.ts",
      "import": "./esm/runtime.js",
      "default": "./esm/runtime.js"
    },
    "./*": "./*"
  },
  "main": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "esm",
    "!esm/**/*.map"
  ],
  "dependencies": {
    "@ice/runtime": "^1.2.4",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.18.9",
    "@ice/bundles": "^0.1.10",
    "babel-plugin-transform-jsx-stylesheet": "1.0.6",
    "consola": "^2.15.3",
    "css": "^2.2.1",
    "lodash.merge": "^4.6.2",
    "rax-compat": "^0.2.1",
    "style-unit": "^3.0.5",
    "stylesheet-loader": "^0.9.1"
  },
  "devDependencies": {
    "@ice/app": "^3.2.1",
    "webpack": "^5.86.0"
  },
  "repository": {
    "type": "http",
    "url": "https://github.com/alibaba/ice/tree/master/packages/plugin-canvas"
  },
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc"
  },
  "publishConfig": {
    "access": "public"
  }
}
