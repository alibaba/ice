    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-app-rewired": "^1.5.2",
    "react-scripts": "^1.1.4",
    "sass-loader": "^7.0.3",
    "webpack-plugin-import": "^0.1.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
