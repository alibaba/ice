{
  "name": "@icedesign/monitor-dashboard",
  "version": "1.0.2",
  "description": "该模板适用于数据监控展示类的管理后台，内置通用的丰富的图表，使用时需要根据需求进行删除和添加",
  "homepage": "https://unpkg.com/@icedesign/monitor-dashboard@latest/build/index.html",
  "files": [
    "src/",
    "build/",
    "public/",
    "tests/",
    "_gitignore",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc"
  ],
  "keywords": [
    "ice-scaffold"
  ],
  "scripts": {
    "start": "ice dev",
    "build": "ice build",
    "lint": "eslint . --ext '.js,.jsx' --fix"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com",
    "access": "public"
  },
  "dependencies": {
    "@antv/data-set": "^0.9.6",
    "@icedesign/base": "^0.2.0",
    "@icedesign/container": "^0.1.4",
    "@icedesign/form-binder": "^1.0.3",
    "@icedesign/img": "^0.1.1",
    "@icedesign/layout": "^0.1.2",
    "@icedesign/menu": "^0.1.1",
    "@icedesign/skin": "^0.1.0",
    "bizcharts": "^3.2.2",
    "classnames": "^2.2.5",
    "foundation-symbol": "^0.1.3",
    "prop-types": "^15.5.8",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ice-scripts": "^1.6.0-next.3"
  },
  "buildConfig": {
    "theme": "@icedesign/skin",
    "entry": "src/index.js",
    "localization": false,
    "output": {
      "publicPath": "./"
    }
  },
  "themeConfig": {
    "primaryColor": "#447eff"
  },
  "scaffoldConfig": {
    "builder": "ice-scripts",
    "name": "ice-monitor-dashboard",
    "title": "数据监控系统",
    "categories": [
      "行业领域"
    ],
    "screenshot": "https://img.alicdn.com/tfs/TB1XiHdAmrqK1RjSZK9XXXyypXa-2872-1580.png"
  }
}
