{
  "name": "@icedesign/iceworks-homepage-scaffold",
  "version": "2.0.0",
  "description": "该模板适用于工具类产品的首页介绍，默认内置一个页面和不同风格的展示区块",
  "homepage": "https://unpkg.com/@icedesign/iceworks-homepage-scaffold@latest/build/index.html",
  "files": [
    "src/",
    "build/",
    "public/",
    "tests/",
    "_gitignore",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc"
  ],
  "scripts": {
    "start": "ice dev",
    "build": "ice build",
    "lint": "eslint . --ext '.js,.jsx' --fix"
  },
  "dependencies": {
    "@alifd/next": "^1.x",
    "@icedesign/theme": "^0.1.0",
    "@icedesign/container": "^1.x",
    "@icedesign/layout": "^1.x",
    "moment": "^2.23.0",
    "prop-types": "^15.5.8",
    "rc-queue-anim": "^1.5.0",
    "rc-scroll-anim": "^2.5.1",
    "react": "^16.4.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ice-scripts": "^1.6.0-next.3"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com",
    "access": "public"
  },
  "buildConfig": {
    "theme": "@icedesign/theme",
    "entry": "src/index.js",
    "output": {
      "publicPath": "./"
    }
  },
  "scaffoldConfig": {
    "builder": "ice-scripts",
    "name": "iceworks-homepage",
    "title": "Iceworks 官网",
    "categories": [
      "官网模板"
    ],
    "screenshot": "https://img.alicdn.com/tfs/TB1hRY5qqmWBuNjy1XaXXXCbXXa-2840-1596.png"
  }
}
