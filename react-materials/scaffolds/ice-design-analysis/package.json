{
  "name": "@icedesign/analysis-scaffold",
  "version": "2.0.1",
  "description": "该模板适用于可视化实时数据的大屏展示，默认只有一个页面，内置饼图、柱状图等图形",
  "homepage": "https://unpkg.com/@icedesign/analysis-scaffold@latest/build/index.html",
  "files": [
    "src/",
    "build/",
    "public/",
    "tests/",
    "_gitignore",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc"
  ],
  "dependencies": {
    "@alifd/next": "^1.11.8",
    "@icedesign/container": "^1.0.0",
    "@icedesign/theme": "^0.1.0",
    "echarts": "^4.0.4",
    "echarts-for-react": "^2.0.8",
    "moment": "^2.24.0",
    "prop-types": "^15.5.8",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.2.2",
    "react-text-loop": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ice-scripts": "^1.6.0-next.3"
  },
  "scripts": {
    "start": "ice dev",
    "build": "ice build",
    "lint": "eslint . --ext '.js,.jsx' --fix"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com",
    "access": "public"
  },
  "buildConfig": {
    "theme": "@icedesign/theme",
    "entry": "src/index.js",
    "localization": false,
    "output": {
      "publicPath": "./"
    }
  },
  "scaffoldConfig": {
    "builder": "ice-scripts",
    "name": "ice-design-analysis",
    "title": "电商类数据大盘",
    "categories": [
      "数据可视化"
    ],
    "screenshot": "https://img.alicdn.com/tfs/TB1mvjPzNTpK1RjSZFGXXcHqFXa-2872-1580.png"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
