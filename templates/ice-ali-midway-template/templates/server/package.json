{
  "name": "ice-midway-react-scaffold",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@ali/midway": "^6.5.7",
    "@ali/pandora": "^3.10.2"
  },
  "devDependencies": {
    "@ali/ci": "^4.7.0",
    "@ali/midway-bin": "6",
    "@ali/midway-mock": "6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.5",
    "ts-node": "^7.0.0",
    "tslib": "^1.8.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "install-alinode": "3.11.7"
  },
  "scripts": {
    "client": "ice dev --project-type=midway",
    "server": "NODE_ENV=local midway-bin dev --ts",
    "start": "npm run client & npm run server",
    "lint": "eslint . --ext '.js,.jsx' --fix",
    "build": "ice build --project-type=node && npm run server-build",
    "start-production": "npm run client & npm run server-start",
    "server-debug": "NODE_ENV=local midway-bin debug --ts",
    "server-test": "npm run lint -- --fix && midway-bin test --ts",
    "server-lint": "tslint -c tslint.json --project .",
    "server-cov": "midway-bin cov --ts",
    "server-ci": "midway-bin cov --ts",
    "server-build": "midway-bin build -c",
    "server-start": "NODE_ENV=local pandora dev"
  },
  "ci": {
    "version": "8, 10"
  },
  "config": {
    "build": {
      "deps": "isolation"
    }
  },
  "midway-bin-build": {
    "include": [
      "app/public",
      "app/view"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
