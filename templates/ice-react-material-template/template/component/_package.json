{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "files": [
    "demo/",
    "lib/",
    "build/"
  ],
  "main": "lib/index.js",
  "stylePath": "style.js",
  "scripts": {
    "start": "ice-scripts dev",
    "build": "ice-scripts build",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "keywords": [
    "ice",
    "react",
    "component"
  ],
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "devDependencies": {<%- typeof adaptor !== 'undefined' && adaptor ? '\n    "@alifd/adaptor-generate": "^0.1.3",' : '' %>
    "ice-plugin-component": "^0.1.1",
    "ice-plugin-fusion": "^0.1.4",
    "ice-scripts": "^2.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "componentConfig": {
    "name": "<%= name %>",
    "title": "<%= title %>",
    "categories": <%- JSON.stringify(Object.keys(categories)) %>
  }
}