{
  "name": "{{npmName}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "files": ["demo/", "lib/"],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba/ice"
  },
  "scripts": {
    "start": "ice-devtools start",
    "build": "ice-devtools build",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "keywords": ["ice", "react", "component"],
  "publishConfig": {
    "access": "public",
    "registry": "{{registry}}"
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "componentConfig": {
    "name": "{{name}}",
    "title": "{{title}}",
    "categories": [
      {{#each categories}}
        "{{@key}}"{{#if @last}} {{else}},{{/if}}
      {{/each}}
    ]
  }
}
