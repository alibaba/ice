{
  "name": "iceworks",
  "private": true,
  "version": "0.0.0",
  "description": "ICE Desktop Application.",
  "scripts": {
    "rebuild-node-pty": "electron-rebuild -f -w app/node_modules/node-pty -m app",
    "appInstall": "cd app && yarn install",
    "rendererIntall": "cd renderer && yarn install",
    "postinstall": "npm run appInstall && npm run rendererIntall",
    "start": "electron app",
    "dev": "gulp dev",
    "build": "gulp build",
    "dist": "gulp dist",
    "build-dist": "gulp build-dist",
    "updates": "gulp publish",
    "upload": "gulp oss",
    "upload-log": "gulp oss-log",
    "lint": "npm run lint:render && npm run lint:main",
    "lint:render": "eslint -c ../../.eslintrc ./renderer/src",
    "lint:main": "eslint -c ../../.eslintrc ./app/main ",
    "icons": "electron-icon-maker --input=./build/1024x1024.png --output=./build/",
    "iconutil": "iconutil -c icns -o ./build/icons/mac/icon.icns ./build/png.iconset"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba/ice"
  },
  "keywords": [
    "ice",
    "iceworks"
  ],
  "license": "MIT",
  "devDependencies": {
