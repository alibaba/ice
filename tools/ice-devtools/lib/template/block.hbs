  {{/if}}
</body>

<script src="{{ blockJS }}"></script>
<script>
function getDefaultOrSelf(mod) {
  if (mod && mod.default) {
    return mod.default;
  } else {
    return mod;
  }
}

  {{#if isReact}}
    {{> initReact }}
  {{/if}}

  {{#if isVue}}
    {{> initVue }}
  {{/if}}
</script>

